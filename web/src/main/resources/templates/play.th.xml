<thlogic>
    <!-- Head -->
    <attr sel="#head" th:replace="~{fragments/head/head :: headFragment}" />
    <!-- Navbar -->
    <attr sel="#navbar" th:replace="~{fragments/navbar/navbar :: navbarFragment}" />
    <!-- Game title -->
    <attr sel="#game-title" th:replace="~{fragments/game-title/game-title :: gameTitleFragment}" />
    <!-- Main message -->
    <attr sel="#main-message" th:replace="~{fragments/main-message/main-message :: mainMessageFragment}" />
    <!-- Result message -->
    <attr sel="#result-message" th:replace="~{fragments/result-message/result-message :: resultMessageFragment}" />
    <!-- Game input -->
    <attr sel="#game-input" th:replace="~{fragments/game-input/game-input :: gameInputFragment}" />
    <!-- Footer -->
    <attr sel="#footer" th:replace="~{fragments/footer/footer :: footerFragment}" />
</thlogic>